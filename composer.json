{
    "name": "uruloke/lara-calendar",
    "type": "library",
    "description": "Recurring calendar events for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "calendar",
        "recurrences"
    ],
    "require": {
        "php": ">=7.0",
        "nesbot/carbon": "^1.22",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "~6.0"
    },
    "authors": [
        {
            "name": "Oliver Nybroe",
            "email": "olivernybroe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Uruloke\\LaraCalendar\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Uruloke\\LaraCalendar\\Test\\": "Tests/"
        }
    }
}
